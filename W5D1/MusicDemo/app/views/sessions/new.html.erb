<h1>Sign Back In</h1>



<%= flash[:errors] %>

<form action="<%= session_url %>" method="post">
	<%= auth_token %>

	<label for="user-email">Email:</label>
	<input id="user-email" type="text" name="user[email]">

	<label for="user-pass">Password:</label>
	<input id="user-pass" type="password" name="user[password]">

	<input type="submit">
	
</form>

<div id="sign_up">
	<h1> Not A Member?, Sign up!</h1>
	<%= link_to 'Go to signup page', new_user_url %>
</div>